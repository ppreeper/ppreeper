  height: 58px;
  margin-left: 6px; }

nav.horizontal ul li {
  display: inline-block; }

.header {
  background-color: white;
  width: 100%; }

.navbar {
  height: 6em;
  display: grid;
  grid-template-columns: 50vw 50vw;
  grid-template-areas: "navleft navright";
  align-items: center;
  background-color: white; }
  .navbar .navleft {
    background-color: white;
    grid-area: "navleft";
    text-align: center; }
    .navbar .navleft img {
      max-width: 300px;
      height: 100%;
      width: 100%;
      object-fit: contain; }
  .navbar .navright {
    background-color: white;
    grid-area: "navright";
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
    justify-content: center;
    font-size: 1.1rem; }
    .navbar .navright a {
      margin: 0.5em;
      text-decoration: none;
      color: darkslategrey; }
      .navbar .navright a :visited,
      .navbar .navright a :active,
      .navbar .navright a :hover {
        color: darkslategrey; }

.carousel {
  background-color: palegoldenrod;
  max-height: 540px;
  width: 100%; }
  .carousel img {
    max-height: 540px;
    width: 100%;
    object-fit: contain; }

.categories {
  text-align: center;
  padding: 1em; }
  .categories h1 {
    font-weight: bolder; }

.indexproducts {
  padding-left: 5vw;
  padding-right: 5vw;
  display: grid;
  gap: 1rem;
  grid-template-columns: repeat(auto-fill, minmax(240px, 1fr)); }
  .indexproducts .indexcat {
    display: flex;
    flex-direction: column;
    justify-content: center;
    align-items: center;
    text-align: center; }
    .indexproducts .indexcat .catimg img {
      max-width: 300px;
      max-height: 300px; }
    .indexproducts .indexcat .cattxt {
      font-size: 1.5em;
      font-weight: 800; }
      .indexproducts .indexcat .cattxt a {
        text-decoration: none;
        color: black; }
        .indexproducts .indexcat .cattxt a :visited,
        .indexproducts .indexcat .cattxt a :active,
        .indexproducts .indexcat .cattxt a :hover {
          color: black; }

.prodgroups {
  padding: 1rem;
  display: grid;
  gap: 1rem;
  grid-template-columns: repeat(auto-fill, minmax(240px, 1fr)); }

.footer-bottom {
  color: white;
  background-color: #385886;
  background-color: rgba(56, 88, 134, 0.8);
  padding: 15px 0 15px 30px;
  text-align: left; }

.footer-contact {
  padding: 50px 0;
  text-align: center;
  background-color: #385886 !important;
  color: white;
  font-size: 18px; }
  .footer-contact a {
    padding: 0.5em 1em;
    color: white;
    font-size: 18px;
    background-color: green;
    border: none;
    text-transform: uppercase;
    text-decoration: none;
    border-radius: 5px; }

.product {
  padding-left: 5vw;
  padding-right: 5vw;
  display: grid;
  grid-template-columns: 1fr 3fr;
  grid-template-areas: "front detail";
  grid-gap: 2em; }
  .product .divleft {
    grid-area: front; }
    .product .divleft h2 {
      background-color: steelblue;
      color: white;
      padding: 0.2em 0.5em; }
    .product .divleft img {
      max-width: 20vw;
      height: auto;
      object-fit: contain; }
    .product .divleft a {
      padding: 5px 18px 5px 18px;
      color: black;
      font-size: 18px;
      background-color: lightgray;
      border: none;
      border-radius: 2px;
      text-transform: uppercase;
      text-decoration: none; }
      .product .divleft a :visited,
      .product .divleft a :active,
      .product .divleft a :hover {
        color: black; }
  .product .divright {
    padding-left: 1em;
    padding-right: 1em;
    grid-area: detail;
    column-count: 2; }
    .product .divright h2 {
      background-color: lightgray;
      padding: 0.2em 0.5em; }

.prodtitle {
  background-color: lightgray;
  text-align: center;
  padding: 1em; }
  .prodtitle .title {
    padding: 1rem;
    font-size: 2.5em;
    font-weight: 900; }

a:link,
a:visited {
  color: #008dd5;
  text-decoration: none; }

a:hover {
  color: #a887be;
  text-decoration: underline; }

a.button {
  color: white;
  text-decoration: none;
  background-color: #4cb037;
  padding: 0.5rem 1rem;
  border-radius: 1rem; }

a.button:hover {
  background-color: #60e146; }

