.webicon.twitter {
  background: url("/assets/images/webicons/webicon-twitter.svg"); }

.webicon.vimeo {
  background: url("/assets/images/webicons/webicon-vimeo.svg"); }

.webicon.youtube {
  background: url("/assets/images/webicons/webicon-youtube.svg"); }

.startup-edmonton {
  text-indent: -999em;
  background: url("/assets/images/startup_edmonton.png");
  display: inline-block;
  width: 155px;
